  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Main",
      "request": "launch",
      "mainClass": "frc.robot.Main",
      "projectName": "vscode-workspace-2023",
      "env": {
         "PATH": "${workspaceFolder}\\build\\jni\\release;C:\\Windows\\system32\\",
         "HALSIM_EXTENSIONS": "${workspaceFolder}\\build\\jni\\release\\halsim_gui.dll;"
      },
    },
    {
      "type": "java",
      "name": "Robot",
      "request": "launch",
      "mainClass": "Main"
    },
    {
      "type": "wpilib",
      "name": "WPILib Desktop Debug",
      "request": "launch",
      "desktop": true
    },
    {
      "type": "wpilib",
      "name": "WPILib roboRIO Debug",
      "request": "launch",
      "desktop": false
    }
  ]
}
